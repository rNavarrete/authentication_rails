<h1>Sign Up</h1>
<%=form_for User.new do |f| %>
<%=f.label :email %>
<%=f.text_field :email %>
<%=f.label :full_name %>
<%=f.text_field :full_name %>
<%=f.label :display_name %>
<%=f.text_field :display_name %>
<%=f.label :password %>
<%=f.password_field :password %>
<%=f.label :password_confirmation %>
<%=f.password_field :password_confirmation %>
<%=f.submit "Sign Up" %>
<%end%>